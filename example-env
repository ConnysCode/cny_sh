MONGO_HOST=[ip_of_mongodb_instance] # only used in dev mode
MONGO_POST=[port_of_mongodb_instance] # optional | default is 27017
MONGO_INITDB_ROOT_USERNAME=[the_intended_username]
MONGO_INITDB_ROOT_PASSWORD=[the_intended_password]

DOMAIN=[the_root_domain_of_the_app] # Important: Without a trailing slash
# For example https://cny.sh 